{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "off",
        "no-useless-catch": "off"
    },
    "globals": {
        "HttpStatus": true,
        "process": true,
        "global": true,
        "db": true,
        "body": true
    }
}
